{
  "name": "create-t3-chill-app",
  "version": "1.0.0",
  "description": "Build a project with Next.js, Supabase, tRPC, Prisma, and Stripe using the Create T3 Chill App",
  "main": "dist/index.js",
  "bin": {
    "create-t3-chill-app": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "template"
  ],
  "scripts": {
    "build": "tsx build.ts",
    "dev": "tsx src/index.ts",
    "create-t3-chill-app": "npm run build && node dist/index.js my-t3-chill-app",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "supabase",
    "trpc",
    "prisma",
    "stripe",
    "typescript",
    "fullstack",
    "starter",
    "template"
  ],
  "author": "Yasindu Nethmina",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.19.11",
    "@types/prompts": "^2.4.9",
    "tsx": "^4.16.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
